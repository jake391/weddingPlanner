@model ViewModel

<div class="row">
    
    <div class="col-md-6">
        <h2>@ViewBag.wedderOne and @ViewBag.wedderTwo Wedding</h2>
    </div>
    <div class="col-md-3"></div>
    <div class="col-md-3">
        <a class="btn btn-outline-dark" asp-action="Dashboard" role="button">Dashboard</a>
        <a class="btn btn-outline-secondary" asp-action="logOut" role="button">Log Out</a>
    </div>
</div>
<br>
<div class="row">
    <div class="col">
        <h4>Date: @ViewBag.date.ToString("MMM dd yyyy")</h4>
    </div>
    <div class="col"></div>
</div>
<br>
<div class="row">
    <div class="col">
        <h4>Guests:</h4>
        <br>
        <ul>
        @{foreach(Rsvp user in @Model.Going){
                <li>@user.User.first_name @user.User.last_name</li>
            }
        } 
        </ul>
    </div>
</div>